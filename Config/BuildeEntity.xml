<?xml version="1.0" encoding="utf-8"?>
<BuildeEntity>
	<BuildeTypies>
		<BuildeType>
			<Id>Services</Id>
			<Name>服务</Name>
			<Description>操作数据库，增删改查</Description>
			<BuildeItems>
				<BuildeItem>
					<Id>Service</Id>
					<Name>搭建框架</Name>
					<Description>搭建一个服务的框架</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>CreateInsert</Id>
					<Name>插单</Name>
					<Description>提供查单实例</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>CreateTools</Id>
					<Name>工具类</Name>
					<Checked>True</Checked>
					<Description>提供常用的方法</Description>
				</BuildeItem>
			</BuildeItems>
		</BuildeType>
		<BuildeType>
			<Id>InterCeptor</Id>
			<Name>切片</Name>
			<Description>干涉程序运行时，加入自定义功能</Description>
			<BuildeItems>
				<BuildeItem>
					<Id>ISaveServiceEvents</Id>
					<Name>保存</Name>
					<Description>涉及保存操作时</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>IConfirmServiceEvents</Id>
					<Name>审核</Name>
					<Description>涉及审核操作时</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>IDisconfirmServiceEvents</Id>
					<Name>撤销审核</Name>
					<Description>涉及撤销审核操作时</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>ValidatorBase</Id>
					<Name>校验</Name>
					<Description>涉及校验操作时</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>DataEntityChangedInterceptor</Id>
					<Name>数据跟踪</Name>
					<Description>涉及数据跟踪操作时，字段改变带值，界面实体刷新</Description>
				</BuildeItem>
				<BuildeItem>
					<Id>EditControllerCommandBase</Id>
					<Name>菜单</Name>
					<Description>涉及菜单操作时，添加按钮，按钮隐藏，按钮逻辑更改</Description>
				</BuildeItem>
				
			</BuildeItems>
		</BuildeType>
	</BuildeTypies>
</BuildeEntity>	
